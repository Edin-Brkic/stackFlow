{
  "extends": [
    "next",
    "next/core-web-vitals",
    "standard",
    "plugin:tailwindcss/recommended",
    "prettier"
  ],
  "overrides": [
    {
      "files": ["**/*.ts", "**/*.tsx"],
      "rules": {
        "no-unused-vars": "off"
      }
    },
    {
      "files": ["**/*.tsx"],
      "rules": {
        "camelcase": "off"
      }
    }
  ]
}
